let t1 = createTable(
    ${
        name: string,
        age: int,
        salary: string
    },
    [("Alice", 27, "$60000"),
     ("Bob", 45, "$100000"),
     ("Mark", 23, "$45000")]
);

let t2 = createTable(
    ${
        name: string,
        state: string
    },
    [("Bob", "Idaho"),
     ("Jack", "California"),
     ("Alice", "Texas"),
     ("Henry", "Indiana")]
);

t1 |> join(t2, ["left.name", "left.age", "right.state"], "left.name == right.name and left.age in 25~45")
|> select(["name"], """state == "Texas"""")
|> collect()
|> println